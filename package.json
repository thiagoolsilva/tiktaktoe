{
  "name": "tiktaktoe",
  "version": "1.0.0",
  "description": "Tik Tak Toe Game",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/tsc -p tsconfig.json",
    "watch": "./node_modules/.bin/tsc -p tsconfig.json -w",
    "remove-dist": "npx rimraf ./dist",
    "remove-coverage": "npx rimraf ./coverage",
    "remove-node-modules": "npx rimraf ./coverage",
    "clean-all": "yarn remove-dist && yarn remove-coverage && yarn remove-node-modules",
    "test": "./node_modules/.bin/jest ",
    "test:specific-tests": "./node_modules/.bin/jest --findRelatedTests ",
    "code-formatter": "yarn prettier --write .",
    "prehusky": "./node_modules/.bin/rimraf .husky",
    "husky": "./node_modules/.bin/husky install",
    "posthusky": "./node_modules/.bin/ts-node scripts/src/husky/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thiagoolsilva/tiktaktoe.git"
  },
  "author": "Thiago Lopes da Silva <thiagoolsilva@gmail.com>",
  "bugs": {
    "url": "https://github.com/thiagoolsilva/tiktaktoe/issues"
  },
  "homepage": "https://github.com/thiagoolsilva/tiktaktoe#readme",
  "devDependencies": {
    "@types/fs-readdir-recursive": "1.0.0",
    "@types/jest": "27.0.1",
    "@types/shelljs": "0.8.9",
    "@types/yargs": "17.0.3",
    "fs-readdir-recursive": "1.1.0",
    "husky": "7.0.2",
    "jest": "27.2.0",
    "prettier": "2.4.1",
    "pretty-quick": "3.1.1",
    "rimraf": "3.0.2",
    "shelljs": "0.8.4",
    "ts-jest": "27.0.5",
    "ts-node": "10.2.1",
    "typescript": "4.4.3"
  },
  "dependencies": {
    "reflect-metadata": "0.1.13",
    "tsyringe": "4.6.0"
  }
}
