{
  "name": "tiktaktoe",
  "version": "1.0.0",
  "description": "Tik Tak Toe Game",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/tsc -p tsconfig.json",
    "watch": "./node_modules/.bin/tsc -p tsconfig.json -w",
    "remove-dist": "./node_modules/.bin/rimraf ./dist",
    "test": "./node_modules/.bin/jest ",
    "run-specific-tests": "./node_modules/.bin/jest --findRelatedTests ",
    "prepare-husky": "./node_modules/.bin/husky install && ./node_modules/.bin/husky add .husky/pre-push 'yarn test'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thiagoolsilva/tiktaktoe.git"
  },
  "author": "Thiago Lopes da Silva <thiagoolsilva@gmail.com>",
  "bugs": {
    "url": "https://github.com/thiagoolsilva/tiktaktoe/issues"
  },
  "homepage": "https://github.com/thiagoolsilva/tiktaktoe#readme",
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "husky": "7.0.2",
    "jest": "27.2.0",
    "rimraf": "3.0.2",
    "ts-jest": "27.0.5",
    "ts-node": "10.2.1",
    "typescript": "4.4.3"
  },
  "dependencies": {
    "reflect-metadata": "0.1.13",
    "tsyringe": "4.6.0"
  }
}
